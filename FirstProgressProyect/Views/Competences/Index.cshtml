@model IEnumerable<FirstProgressProyect.Models.Competence>

@{
    ViewData["Title"] = "Index";

    <h4 class="text-center" style="color: #FFFF">Competencias</h4>
    <hr />
}
<div id="carouselExampleIndicators" class="carousel slide mb-6" data-bs-ride="carousel">
    <div class="carousel-indicators">
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="3" aria-label="Slide 4"></button>
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="4" aria-label="Slide 5"></button>
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="5" aria-label="Slide 6"></button>
    </div>
    <div class="carousel-inner text-center">
        <div class="carousel-item active">
            <img src="https://dce-documents.s3.amazonaws.com/styles/max_650x650/s3/2024-08/group_5200_2.png.webp?itok=hcvMnNlM" class="d-block mx-auto" style="width: 350px; height: 350px;" alt="Primera imagen">
        </div>
        <div class="carousel-item">
            <img src="https://dce-documents.s3.amazonaws.com/styles/max_650x650/s3/2024-08/group_5200.jpg.webp?itok=wMx9a9kY" class="d-block mx-auto" style="width: 350px; height: 350px;" alt="Segunda imagen">
        </div>
        <div class="carousel-item">
            <img src="https://dce-documents.s3.amazonaws.com/styles/max_650x650/s3/2024-08/group_5200_1.png.webp?itok=V6gNmMxg" class="d-block mx-auto" style="width: 350px; height: 350px;" alt="Tercera imagen">
        </div>
        <div class="carousel-item">
            <img src="https://dcp-portal-new.staging-otb.com/sites/default/files/styles/4_3_desktop/public/imagenes-cabecera-tarjetas/cm.jpg.webp?itok=8PUXw0Ag" class="d-block mx-auto" style="width: 350px; height: 350px;" alt="Cuarta imagen">
        </div>
        <div class="carousel-item">
            <img src="https://dce-documents.s3.amazonaws.com/s3fs-public/styles/cuatro_tercios_m_1024px/public/2022-07/recompensas-beneficios-diners-club-miles.jpg?VersionId=BHpXQERTZbUT8_vGSrzAgYB3oZprty7O&itok=Q4r7wG7W" class="d-block mx-auto" style="width: 350px; height: 350px;" alt="Quinta imagen">
        </div>
        <div class="carousel-item">
            <img src="https://old.elcom.expertido.org/wp-content/uploads/2023/08/DinerClubMiles-1024x679.jpg " class="d-block mx-auto" style="width: 350px; height: 350px;" alt="Sexta imagen">
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Anterior</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Siguiente</span>
    </button>
</div>
    <div style=" background-color: rgba(23,50,88,0.8); min-height: 100vh; padding: 20px"
    <p>
    <a asp-action="Create"class="btn btn-primary">Crear Competencias</a>
    </p>


<table class="table" style="color: #FFFF">
    <thead>
        
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Admin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumParticipants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaxNumParticipants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MinNumParticipants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaxDateOfIncription)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Category.Type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Admin.ConfirmPassword)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NumParticipants)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MaxNumParticipants)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MinNumParticipants)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MaxDateOfIncription)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
      
}
    </tbody>
</table>
</div>
